{
  "name": "create-wagmi",
  "description": "Create Wagmi apps with one command",
  "version": "2.0.0-alpha.5",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wagmi-dev/wagmi.git",
    "directory": "packages/create-wagmi"
  },
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo config plugins",
    "build": "pnpm run clean && pnpm run build:esm+types",
    "build:esm+types": "tsc --project tsconfig.build.json --module es2020 --outDir ./dist/esm --declaration --declarationMap --declarationDir ./dist/types",
    "build:templates": "bun .scripts/getTemplates.ts",
    "test:build": "publint --strict",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.test-d.ts"
  ],
  "bin": {
    "wagmi": "./dist/esm/cli.js"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/esm/exports/index.js",
  "types": "./dist/types/exports/index.d.ts",
  "typings": "./dist/types/exports/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/exports/index.d.ts",
      "default": "./dist/esm/exports/index.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "cpy": "^9.0.1",
    "cross-spawn": "^7.0.3",
    "detect-package-manager": "^2.0.1",
    "execa": "^6.1.0",
    "fs-extra": "^10.1.0",
    "ora": "^6.1.2",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2",
    "update-check": "^1.5.4",
    "validate-npm-package-name": "^4.0.0"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/eslint": "^8.4.6",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.7",
    "@types/prompts": "^2.4.1",
    "@types/validate-npm-package-name": "^4.0.0"
  },
  "contributors": ["awkweb.eth <t@wagmi.sh>", "jxom.eth <j@wagmi.sh>"],
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/wagmi-dev"
    }
  ],
  "keywords": ["wagmi", "eth", "ethereum", "dapps", "wallet", "web3", "cli"]
}
